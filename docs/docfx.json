{
  "metadata": [
    {
      "src": [
        {
          "cwd": "..",
          "files": [
            "src/**.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "docs/_site/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/**.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
      {
        "src": "../",
        "files": [
          "CHANGELOG.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "globalMetadata": {
      "_appTitle": "NextAudio Documentation",
      "_appFooter": "NextAudio (c) 2022",
      "_appLogoPath": "images/icon.png",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "dev_langs": [
        "csharp"
      ],
      "_displayLangs": [
        "csharp"
      ]
    },
    "template": [
      "default",
      "templates/darkfx",
      "templates/last-modified"
    ],
    "postProcessors": [
      "ExtractSearchIndex",
      "LastModifiedPostProcessor"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
